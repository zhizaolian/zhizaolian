<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="nju.zhizaolian.fragments.CheckFrontMoneyFragment">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

             <include layout="@layout/money_layout"/>
                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/ensure_receive_money"
                    android:id="@+id/ensure_deposit_money_button"/>
                <Button
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/unable_receive_money"
                    style="@style/MyWarningButtonStyle"
                    android:id="@+id/unable_deposit_money_button"/>


        </LinearLayout>
    </ScrollView>


</FrameLayout>
